# --- Admin Panel Server ---
PORT=3000
MONGO_URI=mongodb+srv://<USERNAME>:<PASSWORD>@<CLUSTER>.mongodb.net/solomon
SESSION_SECRET=<SUPER_RANDOM_SECRET>

# Optional: shared dev/testing key
CUSTOMER_KEY=dev-shared-key

# --- Tenant Map (comma-separated list of tenant slugs) ---
TENANTS=tenant_one,tenant_two

# --- Per-tenant keys (must match Solomon ADMIN_KEY) ---
TENANT_ONE_KEY=<tenant-one-admin-key>
TENANT_TWO_KEY=<tenant-two-admin-key>
